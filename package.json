{
  "name": "notion-automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "build": "tsup",
    "package": "cd dist && zip -r ../lambda.zip ./*",
    "deploy": "npm run build && npm run package && aws lambda update-function-code --function-name YOUR_FUNCTION_NAME --zip-file fileb://lambda.zip"
  },
  "author": "znehraks",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@notionhq/client": "^2.2.16",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/node": "^22.13.9",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8"
  }
}
